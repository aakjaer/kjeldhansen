{
	
	"include": [".htaccess"],
	"exclude": ["node_modules","bower_components/popper.js/docs","bower_components/bootstrap/index.html","bower_components/bootstrap/site","bower.json","Gemfile","Gemfile.lock","gulpfile.js","package-lock.json","package.json","README.md"],
	
	"collections": {"posts": {
				"output": true,
				"_sort-key": null,
				"_subtext-key": null,
				"_image-key": null,
				"_image-size": null,
				"_singular-name": null,
				"_singular-key": null,
				"_disable-add": null,
				"_icon": null,
				"_add-options": null
			}},
	
	"input-options": {"layout": {"hidden": true},"page_class": {"hidden": true},"redirect_from": {"hidden": true}},
	"defaults": [{"scope": {"path": "bower_components"},"values": {"sitemap": false}}],
	
	
	
	"paths": {
		"uploads": null,
		"plugins": "_plugins",
		"data": "_data",
		"collections": "",
		"includes": "_includes",
		"layouts": "_layouts"
	},
	
	"select-data": {"author": {"name": "Daniel Aakjær Sørensen"},"github": ["metadata"],"sass": {"style": "compressed","sass_dir": "assets/_scss"},"emails": {"contact": "vvs@kjeldhansen.dk"}},
	"source": "/usr/local/__site/src"
}
